  "description": "this is a template for react project",
  "repository": {
    "type": "github"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack serve --hot",
    "build": "NODE_ENV=development webpack",
    "build:prod": "NODE_ENV=production webpack",
